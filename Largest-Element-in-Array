//Given an array arr[]. The task is to find the largest element and return it.

class Solution {
  public:
    int largest(vector<int> &arr) {
        // code here
        int max_num=arr[0];
        for(int i=0;i<arr.size();i++){
            if(arr[i]>max_num){
                max_num=arr[i];
            }
        }
        return max_num;
    }
};
